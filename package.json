{
    "name": "obsidian-bookshelf",
    "version": "0.17.0",
    "description": "Manage your books and reading progress in Obsidian",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "version": "node version-bump.mjs && git add manifest.json versions.json",
        "lint": "npx eslint --max-warnings=0",
        "style:check": "npx prettier . --check",
        "style:format": "npx prettier . --write",
        "test": "npx vitest run",
        "test:watch": "npx vitest watch",
        "test:coverage": "npx vitest run --coverage",
        "test:mutation": "npx stryker run",
        "test:integration": "node esbuild.integration-test.config.mjs",
        "prepare": "husky",
        "storybook": "storybook dev -p 6006",
        "storybook:build": "storybook build"
    },
    "keywords": [
        "obsidian"
    ],
    "author": "Philip Weinke <its@weph.dev>",
    "license": "MIT",
    "devDependencies": {
        "@chromatic-com/storybook": "^4.1.0",
        "@eslint/compat": "^1.2.6",
        "@eslint/js": "^9.19.0",
        "@storybook/react-vite": "^9.1.2",
        "@stryker-mutator/typescript-checker": "^9.0.1",
        "@stryker-mutator/vitest-runner": "^9.0.1",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/react": "^16.2.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/luxon": "^3.4.2",
        "@types/node": "^22",
        "@types/react": "^19.0.11",
        "@types/react-dom": "^19.0.4",
        "@vitest/coverage-v8": "^3.0.9",
        "builtin-modules": "^5.0.0",
        "esbuild": "^0.25",
        "esbuild-copy-files-plugin": "^1.2.1",
        "esbuild-plugin-replace": "^1.4.0",
        "esbuild-sass-plugin": "^3.3.1",
        "eslint": "^9.19.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-storybook": "^9.1.2",
        "expect": "^29.7.0",
        "globals": "^16.0.0",
        "husky": "^9.1.7",
        "jsdom": "^26.0.0",
        "moment": "^2.30.1",
        "obsidian": "latest",
        "postcss-modules": "^6.0.1",
        "prettier": "^3.5",
        "radashi": "^12.5.1",
        "storybook": "^9.1.2",
        "ts-node": "^10.9.2",
        "tslib": "^2.8.1",
        "typescript": "^5.7.0",
        "typescript-eslint": "^8.22.0",
        "vitest": "^3.0.9",
        "@storybook/addon-docs": "^9.1.2"
    },
    "engines": {
        "node": "^20"
    },
    "eslintConfig": {
        "extends": [
            "plugin:storybook/recommended"
        ]
    },
    "dependencies": {
        "chart.js": "^4.4.7",
        "chartjs-adapter-luxon": "^1.3.1",
        "lucide-react": "^0.507.0",
        "luxon": "^3.5.0",
        "mdast-util-from-markdown": "^2.0.2",
        "obsidian-daily-notes-interface": "^0.9.4",
        "react": "^19.0.0",
        "react-chartjs-2": "^5.3.0",
        "react-dom": "^19.0.0",
        "romans": "^3.0.0",
        "typescript-parsec": "^0.3.4"
    }
}
